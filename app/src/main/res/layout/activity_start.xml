<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/celesteTid"
    tools:context="com.example.constanza.tingoidapp.StartActivity">

    <ImageView
        android:id="@+id/izq"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:src="@drawable/izq"
        android:layout_alignTop="@+id/der"
        android:layout_toStartOf="@+id/der" />

    <ImageView
        android:id="@+id/der"
        android:layout_width="134dp"
        android:layout_height="120dp"
        android:src="@drawable/der"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="55dp" />


</RelativeLayout>
